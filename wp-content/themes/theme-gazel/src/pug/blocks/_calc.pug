                                    input.b-calc-input__input.calc-departure(type="text" name="departure" placeholder="Москва или город МО" value="Россия, Москва")
                                    .b-calc-input__error.error
                            .col.col--lg-1.col--lg-offset-3.col--sm-2.text--xs-center
                                span.b-calc-input-label куда
                            .col.col--lg-8.col--sm-10
                                label.b-calc-input.b-calc-input--indent
                                    +svg('close').icon.icon-close.clear-input
                                    input.b-calc-input__input.calc-destination(type="text", name="destination", placeholder="Введите город МО")
